  "main": "server.js",
  "engines": {
    "node": "12.3.1",
    "npm": "6.9.0"
  },
  "description": "",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.1",
    "express": "^4.17.1",
    "is-empty": "^1.2.0",
